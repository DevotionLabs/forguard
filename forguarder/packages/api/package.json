{
	"name": "api",
	"version": "0.1.0",
	"description": "ForGuard HTTP API",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"start": "node dist/index.js",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write . --config ../../.prettierrc",
		"generate-api-routes": "openapi-generator-cli generate -i ./src/services/docker/engine/spec/v1.43.yaml -g typescript-axios -o ./src/services/docker/engine/generated",
		"download-compose-spec": "wget -O ./src/services/docker/compose/file/spec/compose-spec.json https://raw.githubusercontent.com/compose-spec/compose-spec/main/schema/compose-spec.json",
		"generate-compose-types": "json2ts -i ./src/services/docker/compose/file/spec/compose-spec.json -o ./src/services/docker/compose/file/generated/types.d.ts"
	},
	"author": "Diego (DevotionLabs)",
	"license": "GPL-3.0",
	"dependencies": {
		"axios": "^1.7.7",
		"express": "^4.21.0",
		"js-yaml": "^4.1.0"
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.14.0",
		"@types/express": "^5.0.0",
		"@types/jest": "^29.5.13",
		"@types/js-yaml": "^4.0.9",
		"@types/supertest": "^6.0.2",
		"jest": "^29.7.0",
		"jest-express": "^1.12.0",
		"json-schema-to-typescript": "^15.0.2",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5"
	}
}
